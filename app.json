{
  "name": "siege-engine",
  "scripts": {
  },
  "env": {
    "LANG": "en_US.UTF-8",
    "RACK_ENV": "production",
    "RAILS_ENV": "production",
    "RAILS_LOG_TO_STDOUT": "enabled",
    "RAILS_SERVE_STATIC_FILES": "enabled",
    "SECRET_KEY_BASE": {
      "generator": "secret"
    },
    "SE_ACCESS_KEY_ID": {
      "generator": "secret"
    },
    "SE_ACCESS_KEY_SECRET": {
      "generator": "secret"
    },
    "SIDEKIQ": "true",
    "WEBMOCK": "false"
  },
  "addons": [
    "papertrail",
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "environments": {
    "production": {
      "addons": [
        {
          "plan": "heroku-postgresql:standard-1x"
        },
        {
          "plan": "heroku-redis:premium-0"
        },
        {
          "plan": "papertrail:choklad"
        }
      ]
    }
  }
}
